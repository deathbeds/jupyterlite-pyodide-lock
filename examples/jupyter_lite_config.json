{
  "LiteBuildConfig": {
    "cache_dir": "../build/.cache/lite",
    "output_dir": "../build/docs-app"
  },
  "PyodideLockAddon": {
    "enabled": true,
    "locker": "BrowserLocker",
    "specs": ["ipywidgets >=8.1.2,<8.2"]
  },
  "PyodideLockOfflineAddon": {
    "enabled": true,
    "extra_includes": ["^ipywidgets$"]
  },
  "BrowserLocker": {
    "private_mode": true,
    "temp_profile": true
  },
  "WebDriverLocker": {
    "webdriver_log_path": "../build/geckodriver.log",
    "webdriver_service_args": ["--log", "trace"]
  }
}
