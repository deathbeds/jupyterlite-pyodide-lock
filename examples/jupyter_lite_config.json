{
  "LiteBuildConfig": {
    "cache_dir": "../build/.cache/lite",
    "output_dir": "../build/docs-app",
    "apps": ["consoles", "edit", "tree", "lab", "notebooks", "repl", "voici"],
    "strict": true
  },
  "PyodideLockAddon": {
    "enabled": true,
    "locker": "UvLocker",
    "packages": [
      "https://files.pythonhosted.org/packages/py3/i/ipython/ipython-9.8.0-py3-none-any.whl",
      "https://files.pythonhosted.org/packages/py3/m/matplotlib-inline/matplotlib_inline-0.2.1-py3-none-any.whl"
    ],
    "specs": ["ipywidgets >=8.1.2,<8.1.3"],
    "constraints": ["traitlets <5.14.3", "prompt-toolkit >=3.0.52"]
  },
  "FixHtmlAddon": {
    "enabled": true,
    "rewrite_missing": {
      "^(.*)$": "/files/\\1\" target=\"_blank"
    },
    "task_dep": ["post_build:voici:voici:generate:render/README.html"],
    "file_dep": ["voici/render/**/*.html"]
  },
  "PyodideLockOfflineAddon": {
    "enabled": true,
    "extra_includes": ["^ipywidgets$"],
    "extra_excludes": [
      "^.*-test$",
      "^.*-test\\d$",
      "^astropy$",
      "^gdal$",
      "^poalrs$",
      "^opencv-python$",
      "^duckdb$",
      "^bokeh$",
      "^pysam$",
      "^mne$",
      "^fiona$",
      "^pyproj$",
      "^libmagic$",
      "^python-filnt$",
      "^yt$",
      "^statsmodels$"
    ]
  }
}
