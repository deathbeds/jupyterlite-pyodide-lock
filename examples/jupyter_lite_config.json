{
  "LiteBuildConfig": {
    "cache_dir": "../build/.cache/lite",
    "output_dir": "../build/docs-app"
  },
  "PyodideLockAddon": {
    "enabled": true,
    "locker": "browser",
    "specs": [
      "ipywidgets >=8.1,<8.2"
    ]
  },
  "BrowserLocker": {
    "private_mode": true,
    "temp_profile": true
  },
  "WebDriverLocker": {
    "webdriver_log_path": "../build/geckodriver.log",
    "webdriver_service_args": ["--log", "trace"]
  }
}
